<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="demo2.client.MyResources" field="res"/>
	
	<g:HTMLPanel>
		<g:Image url="{res.image.getURL}"></g:Image>
		<g:Image resource="{res.image}"></g:Image>
		<div class="{res.css.redbox}"></div>
	</g:HTMLPanel>
</ui:UiBinder>

